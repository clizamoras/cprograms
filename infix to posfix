#include<stdio.h>
#include<ctype.h>
#define size 50
char s[size];
int top=-1;
void push(char elem){
  s[++top]=elem;
}
char pop(){
  (return(s[top--]));
}
int pr(char elem){
  switch(elem){
    case '#':return 0;
    case '(':return 1;
    case '+':
    case '-':return 2;
    case '*':
    case '/':
    case '%':return 3;
    case '^':return 4;
  }
}
int main(){
char infix[50],pos[50],ch,elem;
int i=0,k=0;
printf("Enter infix");
scanf("%s",infix);
push('#');
while((ch=infix[i++])!='\0'){
  if(ch=='('){
    push('(');
  }
  else if(isalnum(ch)){
   pos[k++]=ch;
  }
  else if(ch==')'){
    while(s[top]!='('){
      pos[k++]=pop();
    }
    elem=pop();
  }
  else{
    while(pr(s[top])>=pr(ch)){
       pos[k++]=pop();
    }
    push(ch);
  }
}
while(s[top]!='#'){
  pos[k++]=pop();
}
pos[k]='\0';
printf("posfix:%s",pos);
return 0;
}
