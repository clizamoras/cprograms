#include <stdio.h>
#include <string.h>

int main() {
    char str1[20] = "ApplePie";
    char str2[20] = "AppleJam";

    // Compare first 5 characters
    if (strncmp(str1, str2, 5) == 0) {
        printf("First 5 characters are the same.\n");
    } else {
        printf("First 5 characters are different.\n");
    }

    return 0;
}
