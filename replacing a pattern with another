
#include <stdio.h>

int main() {
    char s[100], pat[20], rep[20], result[100];
    int i = 0, j = 0, k, flag = 0;

    gets(s);
    gets(pat);
    gets(rep);

    while (s[i] != '\0') {
        flag = 1;
        for (k = 0; pat[k] != '\0'; k++) {
            if (s[i + k] != pat[k]) {
                flag = 0;
                break;
            }
        }

        if (flag) {
            for (k = 0; rep[k] != '\0'; k++)
                result[j++] = rep[k];
            i += k;
        } else {
            result[j++] = s[i++];
        }
    }
    result[j] = '\0';

    printf("Result: %s", result);
    return 0;
}
