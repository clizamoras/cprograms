#include <stdio.h>
#include <string.h>

int main() {
    char source[20] = "OrangeJuice";
    char destination[20];

    // Copy first 6 characters from source to destination
    strncpy(destination, source, 6);
    destination[6] = '\0'; // Null terminate manually

    printf("Copied string: %s\n", destination);

    return 0;
}
