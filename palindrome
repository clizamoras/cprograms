
#include<stdio.h>
int main(){
char arr[100];
int len=0;
int ispalindrome=1;
printf("Enter the string");
gets(str);
while(str[len]!='\0'){
  len++;
}
for (int i=0;i<len/2;i++){
if(str[i]!+str[len-1-i]{
ispalindrome=0;
break;
}
}
if(ispalindrome=1){
printf("is palindrome");}
else{
printf("Not");
}
return 0;
}
