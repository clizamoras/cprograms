
#include <stdio.h>

int main() {
    // Declare and initialize a string array with 3 strings
    char fruits[3][20] = {"Apple", "Banana", "Cherry"};

    // Print all strings using a loop
    printf("List of Fruits:\n");
    for(int i = 0; i < 3; i++) {
        printf("%s\n", fruits[i]);
    }

    return 0;
}
